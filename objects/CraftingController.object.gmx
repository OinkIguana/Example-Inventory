        if(craft_get(i, CRAFTABLE)) {
            optionCount++;
        }
    }
    //Loop the menu options
    if(keyboard_check_pressed(vk_up)) {
        currentOption = (currentOption - 1 + optionCount) % optionCount;
    }
    if(keyboard_check_pressed(vk_down)) {
        currentOption = (currentOption + 1) % optionCount;
    }
    //Put the item into the list
    if(keyboard_check_pressed(vk_enter)) {
        var found = 0;
        for(i = 0; i &lt; ds_grid_width(recipes); i++) {
            if(craft_get(i, CRAFTABLE)) {
                found++;
                if(found &gt; currentOption) {
                    break;
                }
            }
        }
        if(craft_queue(craft_get(i, CRAFTNAME))) {
            menuVisible = false;
            game_unpause();
        }
    }
} else {
    currentOption = 0;
}
